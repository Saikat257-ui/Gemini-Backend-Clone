# Database Configuration
DATABASE_URL=your_postgresql_connection_string

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key

# JWT Configuration
JWT_SECRET=your_secure_jwt_secret

# Rate Limiting
